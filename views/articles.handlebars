<div class="parallax-container valign-wrapper">
    <div class="parallax">
        <img src="https://images3.alphacoders.com/220/220047.jpg">
    </div>
</div>
<ul id="list-row" class="collapsible" data-collapsible="accordion">
    <li>
        <div class="collapsible-header center-align">ARCHIVE<i class="material-icons right">swap_vert</i></div>
        <div class="collapsible-body">
            <ul id="archive_list" class="collapsible popout" data-collapsible="accordion"></ul>
        </div>
    </li>
</ul>
<!-- Modal Structure -->
<div id="modal-note" class="modal bottom-sheet">
    <div class="modal-content">
        <div class="row">
            <form class="col s12">
                <div id="note-div" class="row">
                    <div class="col s7">
                        <ul id="note" class="collapsible collection with-header" data-collapsible="accordion">
                        </ul>
                    </div>
                    <div class="col s5">
                        <div class="row">
                            <div class="input-field col s12">
                                <i class="material-icons prefix">subject</i>
                                <input id="note-title" type="text" class="validate">
                                <label for="note-title">Title</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <i class="material-icons prefix">mode_edit</i>
                                <textarea id="note-text" class="materialize-textarea" data-length="200"></textarea>
                                <label for="note-text">Note</label>
                            </div>
                        </div>
                        <div id="note-div2" class="row">
                            <a id="note-submit" class="waves-effect waves-green btn right"><i class="material-icons prefix">input</i></a>
                        </div>
                    </div>
                </div>

            </form>
        </div>

    </div>
</div>

<div class="parallax-container">
    <div class="parallax"><img src="https://images6.alphacoders.com/794/794239.jpg"></div>
</div>